{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "import/prefer-default-export": 0,
    "react/jsx-filename-extension": 0,
    "import/extensions": 0,
    "global-require": 0,
    "no-param-reassign": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "fetch": true
  }
}
